{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "rewrites": [
    {
      "source": "/api/wordsets",
      "destination": "/api-vercel/wordsets"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api-vercel/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
